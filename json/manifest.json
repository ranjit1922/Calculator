{
  "name": "Smart Calculator",
  "short_name": "Calculator",
  "description": "An advanced scientific calculator.",
  "start_url": "https://ranjit1922.github.io/Calculator/",
  "scope": "/",
  "display": "standalone",
  "theme_color": "#d38d4d",
  "background_color": "#ffffff",
  "orientation": "portrait-primary",
  "lang": "en",
  "dir": "ltr",
  "icons": [
    {
      "src": "images/icon/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "images/icon/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "images/icon/maskable-icon.webp",
      "sizes": "512x512",
      "type": "image/webp",
      "purpose": "maskable"
    },
    {
      "src": "images/icon/favicon.ico",
      "sizes": "any",
      "type": "image/x-icon",
      "purpose": "any"
    }
  ],
  "shortcuts": [
    {
      "name": "Open Calculator",
      "short_name": "Open",
      "description": "Open the calculator",
      "url": "index.html",
      "icons": [
        {
          "src": "images/icon/favicon.ico",
          "sizes": "any",
          "type": "image/x-icon"
        }
      ]
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "name",
      "text": "description",
      "url": "share_url",
      "files": []
    }
  },
  "prefer_related_applications": false,
  "id": "smart_calculator",
  "display_override": [
    "standalone",
    "fullscreen",
    "minimal-ui",
    "window-controls-overlay",
    "browser"
  ],
  "categories": [
    "productivity",
    "utilities"
  ],
  "edge_side_panel": {
    "preferred_width": 400
  }
}
